            android:theme="@style/AppTheme1" />
        <activity
            android:name="com.education.Faculty.Activites.Attendence"
            android:theme="@style/AppTheme1" />
        <activity
            android:name="com.education.Faculty.Activites.ChangePassword"
            android:theme="@style/AppTheme1" />
        <activity
            android:name="com.education.Faculty.Activites.DownloadImage"
            android:theme="@style/AppTheme1" />
        <activity
            android:name="com.education.Faculty.Activites.Splach_screen"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.education.Faculty.Activites.LoadMenu"
            android:theme="@style/AppTheme1" />
        <activity android:name="com.education.Faculty.API.Data" />
        <activity
            android:name="com.education.Faculty.Activites.Leave"
            android:theme="@style/AppTheme1" />
        <activity
            android:name="com.education.Faculty.Activites.Dashbord"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize|screenLayout"
            android:theme="@style/AppTheme1"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.education.Faculty.Activites.WelcomeActivity"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.education.Faculty.Activites.MainActivity"
            tools:ignore="LockedOrientationActivity" />
        <activity android:name="com.education.Faculty.Activites.AddMoment"
            android:theme="@style/AppTheme1"/>
        <activity android:name="com.education.Faculty.Activites.Addhomework"
            android:theme="@style/AppTheme1"/>
        <activity android:name="com.education.Faculty.Adapter.MyCustomAdapter"
            android:theme="@style/AppTheme1"/>

